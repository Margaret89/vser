extends ../layout/main

block variables
	- var title = 'Содержание';
	- 
		var pagelist = [
			{href: 'main.html',name:'Главная'},
			{href: 'contacts.html',name:'Контакты'},
			{href: 'services.html',name:'Наши услуги'},
			{href: 'about.html',name:'О нас'},
			{href: 'partners.html',name:'Наши партнеры'},
			{href: 'tipografika.html',name:'Типографика'},
